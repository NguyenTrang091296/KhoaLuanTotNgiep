        //    var myselect = $('<select>');
        //    $.each(resultData, function (index, key) {
        //        myselect.append($('<option></option>').val(key).html(key));
        //    });
        //    $('#selectCity').append(myselect.html());
        //});
    </script>
</body>
</html>