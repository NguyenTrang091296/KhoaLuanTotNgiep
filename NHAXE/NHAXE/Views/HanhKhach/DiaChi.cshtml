<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Select Box</title>
    <link rel="stylesheet" href="http://bootswatch.com/cosmo/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js" type="text/javascript"></script>
    <script src="ajax.js" type="text/javascript"></script>
</head>
<body>
    <div class="container">
        <div class="row">
            <h3 class="text-center">SELECT BOX</h3>
        </div>
        <div class="row">
            <div class="col-md-4 col-md-offset-2">
                <select class="form-control city" id="selectCity" asp-items="(SelectList)ViewBag.tinh" >
                 
                </select>
            </div>
            <div class="col-md-4">
                <select class="form-control huyen" id="selectHuyen">
                    <option value="0">Quận/Huyện</option>

                </select>
            </div>
        </div>
        </br>
        </br>
        </br>
        </br>
    </div>
    <script>

        $(document).ready(function () {
            $(".city").change(function () {
                var id = $(".city").val();
                $.get("HanhKhach/DSHuyen", { id: id }, function (data) {
                    var myselect = $('<select>');
                    $.each(data, function (index, key) {
                        myselect.append($('<option></option>').val(key).html(key));
                    });
                    $('#selectHuyen').append(myselect.html());
                })
            })
        })
        //var resultData = $get("HanhKhach/DSTinh")
